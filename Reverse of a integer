class Solution {
public:
    int reverse(int x) {
        long long temp;
        long long ans=0;
        while(x!=0){
            temp=x%10;
            ans=ans*10+temp;
            x/=10;
        }
        if(ans>INT_MAX || ans<INT_MIN){
            return 0;
        }
        return ans;
    }
};
