class Solution {
public:
    int findLucky(vector<int>& arr) {
        unordered_map<int,int>mp;
        int max_len=-1;
        int n=arr.size();
        for(int i=0; i<n; i++){
            mp[arr[i]]++;
        } 
        for(pair<int,int>p:mp){
            if(p.first==p.second){
                max_len=max(max_len,p.first);
            }
        }
    
        return max_len;
    }
};
