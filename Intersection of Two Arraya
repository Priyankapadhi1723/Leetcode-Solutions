class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int>sset;
        vector<int>result;
        for(auto num:nums1){
            if(!sset.count(num)) sset.insert(num);
        }
        for(auto num:nums2){
            if(sset.count(num)){
                result.push_back(num);
                sset.erase(num);
            }
        }
        return result;
    }
};
