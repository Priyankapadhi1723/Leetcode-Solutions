class Solution {
public:
    int maxSum(vector<int>& nums) {
        int n=nums.size();
        int sum=0;
        unordered_set<int>myset;
        for(int i=0; i<n; i++){
            if(myset.find(nums[i])==myset.end() &&nums[i]>0){
                sum+=nums[i];
            }
            myset.insert(nums[i]);

        }
            if(sum==0){
                sort(nums.begin(),nums.end());
                sum=nums[n-1];
            }
        
        return sum;
    }
};
